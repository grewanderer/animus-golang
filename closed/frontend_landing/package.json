{
  "name": "animus-frontend-landing",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "node scripts/validate-env.mjs --mode=runtime && node node_modules/next/dist/bin/next dev -p 3000 -H 0.0.0.0",
    "build": "node scripts/validate-env.mjs --mode=build && node node_modules/next/dist/bin/next build",
    "start": "node scripts/validate-env.mjs --mode=runtime && node node_modules/next/dist/bin/next start -H 0.0.0.0 -p 3000",
    "lint": "node node_modules/next/dist/bin/next lint",
    "format": "prettier --write .",
    "typecheck": "node node_modules/typescript/bin/tsc --noEmit",
    "clean": "node scripts/clean.mjs"
  },
  "dependencies": {
    "@radix-ui/react-slot": "1.0.2",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "next": "15.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "2.5.2"
  },
  "devDependencies": {
    "@types/node": "20.17.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-config-next": "15.0.3",
    "postcss": "8.4.47",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "3.4.15",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": "20.18.1",
    "npm": "10.9.0"
  },
  "packageManager": "npm@10.9.0"
}
